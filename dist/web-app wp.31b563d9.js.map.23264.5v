  projectDivider.classList.add(projectAnimationClass);\r\n    }\r\n  cardsAnimationDelay(projectCards, projectAnimationClass);\r\n}\r\n\r\n// ---     LIFE ---\r\nfunction lifeAddAnimation() {\r\n  lifeTitle.classList.add(lifeAnimationClass);\r\n  if (lifeDivider && lifeDivider.classList.contains(lifeDividerClass)) {\r\n    lifeDivider.classList.add(lifeAnimationClass);\r\n    }\r\n  lifeSlider.classList.add(lifeAnimationClass);\r\n  lifeSliderDots.classList.add(lifeAnimationClass);\r\n  lifeSliderArrowLeft.classList.add(lifeAnimationClass);\r\n  lifeSliderArrowRight.classList.add(lifeAnimationClass);\r\n}\r\n\r\n// ---     FOOTER ---\r\nfunction footerAddAnimation() {\r\n  footerContainer.classList.add(footerAnimationClass);\r\n}","export const pageBody = document.querySelector(\".page\");\r\n\r\n// --- HEADER ---\r\nexport const header = document.querySelector(\".header\");\r\nexport const headerNavList = document.querySelector(\".header__nav_list\");\r\nexport const burgerButton = document.querySelector(\".header__burger\");\r\nexport const sideNavCloseButton = document.querySelector(\r\n  \".header__nav_list_close-button\"\r\n);\r\nexport const sideBarLanguageLinks = document.querySelector(\r\n  \".topbar__language_links_side-bar\"\r\n);\r\n\r\n// --- ABOUT ---\r\nexport const aboutInfoWrapper = document.querySelector(\".about__info_wrapper\");\r\nexport const aboutInfoWrapperClass = \"about__info_wrapper\";\r\nexport const aboutAnimation = \"about__animation\";\r\nexport const headerFixed = \"header_fixed\";\r\nexport const active = \"active\";\r\nexport const overlay = \"overlay\";\r\n// export let headerFixState = false;\r\n\r\n// --- BACKGROUND ---\r\n// cards\r\nexport const background = document.querySelector(\".background\");\r\nexport const backgroundClass = \"background\";\r\nexport const backgroundItemCheckboxes = document.querySelectorAll(\r\n  \".background__item_checkbox\"\r\n);\r\nexport const backgroundCards = document.querySelectorAll(\".backgroun